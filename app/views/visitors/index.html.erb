	<div id="map" style="width: 1120px; height: 400px;"></div>

  <script type="text/javascript">
    var locations = [	
		['MEDCAN Medical Marijuana Card Certifying Clinic', 30.475671, -84.228009],
		['Medical Marijuana Treatment Clinics of Florida', 30.475310, -84.236089],
		['Trulieve', 30.436531, -84.227296],
		['KNOX Cannabis Dispensary', 30.465380, -84.269931],
		['Dynamic Spine and Wellness Center', 30.468433, -84.23106],
		['Dr Fred E Russo, CCSP', 30.428132, -84.243969],
		['Fiorini Chiropractic Center, P.A.', 30.426601, -84.249447],
		['Pathway Wellness Chiropractic Clinic', 30.456204, -84.237327],
		['Joseph Miller Chiropractic', 30.477556, -84.234706],
		['Dr. Steven Willis', 30.476530, -84.233759],
		['Mc Intosh Clinic: Mc Millan Victor M MD', 30.827273, -83.973088],
		['Paige Apgar Continentino, CPC, EEM-CLP ', 30.485827, -84.228093],
		['Suki Horne Acupuncture Physician', 30.428793, -84.24421],
		['Yisheng Clinic', 30.446326, -84.259981],
		['Acupuncture Pain Relief Center', 30.456720, -84.264025],
		['Dr. Less Shrestha', 30.393253, -84.231814],
		['Patel Pooja N MD', 30.477831, -84.233591],
		['Arthritis Center Of Tallahassee', 30.458906, -84.238519],
		['Adriene D. Hill, DC', 30.219570, -84.361905],
			['Donald J. Van Tassel, DC'	, 30.479036, -84.23834], 
		['Massage Envy', 30.542344, -84.22978]
	];

    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 9,
      center: new google.maps.LatLng(30.4383, -84.2807),
      mapTypeId: google.maps.MapTypeId.ROADMAP
    });

    var infowindow = new google.maps.InfoWindow();

    var marker, i;

    for (i = 0; i < locations.length; i++) {  
      marker = new google.maps.Marker({
        position: new google.maps.LatLng(locations[i][1], locations[i][2]),
        map: map
      });

      google.maps.event.addListener(marker, 'click', (function(marker, i) {
        return function() {
          infowindow.setContent(locations[i][0]);
          infowindow.open(map, marker);
        }
      })(marker, i));
    }
  </script>
